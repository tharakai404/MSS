@model IEnumerable<ABC.Rent>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PeriodsInMonths)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.monthlyRentFee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KeyMoney)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tenant.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResidentialUnit.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PeriodsInMonths)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.monthlyRentFee)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KeyMoney)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tenant.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResidentialUnit.Name)
        </td>
    </tr>
}

</table>
